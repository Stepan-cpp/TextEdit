<lib:Theme xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
           xmlns:lib="clr-namespace:TextLib;assembly=TextLib"
           Name="Dark"
           Author="Default">
    <ResourceDictionary>
        <SolidColorBrush x:Key="TextFieldColor" Color="#202020"/>
        <SolidColorBrush x:Key="ToggleChecked" Color="#252525"/>
        <SolidColorBrush x:Key="Foreground" Color="#CCCCCC"/>
        <SolidColorBrush x:Key="ForegroundDisabled" Color="#707070"/>
        <SolidColorBrush x:Key="Background" Color="#2F2F2F"/>
        <SolidColorBrush x:Key="Lowlight" Color="#2A2A2A"/>
        <SolidColorBrush x:Key="LowlightBackground" Color="#353535"/>
        <SolidColorBrush x:Key="BorderHighlight" Color="#2A2A2A"/>
        <SolidColorBrush x:Key="MouseOver" Color="#292929"/>
    </ResourceDictionary>
</lib:Theme>